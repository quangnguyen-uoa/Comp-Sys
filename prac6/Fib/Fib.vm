// Contains a function Fib.fib that calculates and returns the nth Fibonacci number recursively
// n is supplied as an argument

// Put your code here.

function Fib.fib 3
    push constant 0
    pop local 0
    push constant 1
    pop local 1

label LOOP
    push argument 0
    push constant 1
    lt 
    if-goto END
    push local 0
    push local 1
    add
    pop local 2

    push local 1
    pop local 0
    push local 2
    pop local 1

    push argument 0
    push constant 1
    sub
    pop argument 0
    goto LOOP

label END 
    push local 0
    return


